<script src="/public/plugins/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="/public/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
<script src="/public/plugins/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
