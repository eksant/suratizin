<script>
  $(document).ready(function () {
    $('.sidebar-menu').tree()
    $('#dataTable').DataTable({
      'paging'      : true,
      'lengthChange': true,
      'searching'   : true,
      'ordering'    : true,
      'info'        : true,
      'autoWidth'   : true,
    })

    $('#modal-warning').on('show.bs.modal', function (event) {
      var button    = $(event.relatedTarget)
      var recipient = button.data('message')
      var action    = button.data('action')
      var modal     = $(this)
      var message   = (recipient != '') ? recipient : 'Anda yakin ingin melanjutkan ??'
      modal.find('.modal-title').text(message)
      modal.find('.modal-action').attr('href', action)
    })
  })
</script>
